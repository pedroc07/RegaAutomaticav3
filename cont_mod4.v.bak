//Definição do módulo Contador m´odulo 4
module cont_mod4(Clk, Rst, Q1, Q0);

input Clk, Rst;

output Q1, Q0;

//Fios intermediários
wire Q2n, Q1n, Q0n;

not Not0(Q2n, Q2);
not Not1(Q1n, Q1);
not Not2(Q0n, Q0);

//Flip-flops JK usados para fazer o contador
//FF0
flipflopJK(1, 1, 0, Rst, Clk, Q0);
//FF1
flipflopJK(Q0n, Q0n, 0, Rst, Clk, Q1);

endmodule